#: pathauto.inc:408
msgid "Created new alias %dst for %src, replacing %old_alias. %old_alias now redirects to %dst"
msgstr "Δημιουργήθηκε νέο ψευδώνυμο %dst για το %src, αντικαθιστώντας το %old_alias. Το %old_alias τώρα δείχνει στο %dst."

#: pathauto.inc:411
msgid "Created new alias %dst for %src, replacing %old_alias"
msgstr "Δημιουργήθηκε νέο ψευδώνυμο %dst για το %src, αντικαθιστώντας το %old_alias."

#: pathauto.inc:414
msgid "Created new alias %dst for %src"
msgstr "Δημιουργήθηκε νέο ψευδώνυμο %dst για το %src."

